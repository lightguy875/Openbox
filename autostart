#Execução do compositor
compton --config .config/compton/compton.conf -b &
#iniciar network manager
nm-applet &
#painel1
cairo-dock -o  &
#painel2
#tint2 &
#controle de som do gnome
gnome-sound-applet &
# setar teclado
setxkbmap -layout br -variant abnt2 &
#configuracao para teclar com touchpap
xinput set-prop "DLL0641:00 06CB:7621 Touchpad" "libinput Tapping Enabled" 1 &
#configuracao para iniciar o controle de volume
volumeicon &
#configuração de wallpaper
nitrogen --restore &
#screen-saver
#gnome-screensaver
#gnome-polkit
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &
#Coloração da tela de acordo com o dia
redshift &
